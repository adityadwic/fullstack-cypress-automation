{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "cypress"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:cypress/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "cypress/no-unnecessary-waiting": "warn"
  }
}
